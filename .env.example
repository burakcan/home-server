# Cloudflare and Domain Configuration
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
IMMICH_DOMAIN=photos.yourdomain.com
HOMEPAGE_DOMAIN=home.yourdomain.com
PLEX_DOMAIN=plex.yourdomain.com
QBITTORRENT_DOMAIN=qbit.yourdomain.com
SONARR_DOMAIN=sonarr.yourdomain.com
RADARR_DOMAIN=radarr.yourdomain.com
HOMEASSISTANT_DOMAIN=ha.yourdomain.com

# Media Paths
MEDIA_ROOT=/data
DOWNLOADS_DIR=${MEDIA_ROOT}/downloads
TV_DIR=${MEDIA_ROOT}/tv
MOVIES_DIR=${MEDIA_ROOT}/movies
PHOTOS_DIR=${MEDIA_ROOT}/photos

# Service Configuration
PUID=1000
PGID=1000
TZ=Your/Timezone

# VPN Configuration for qBittorrent
VPN_ENABLED=true
VPN_PROVIDER=generic
VPN_CONF=wg0
VPN_LAN_NETWORK=192.168.0.0/24

# Immich Configuration
UPLOAD_LOCATION=${PHOTOS_DIR}
DB_USERNAME=postgres
DB_PASSWORD=your_secure_password
DB_DATABASE_NAME=immich
DB_DATA_LOCATION=${MEDIA_ROOT}/db/immich
IMMICH_VERSION=release

# Plex Configuration
PLEX_CLAIM=claim-xxxxxxxxxxxxx
PLEX_HOSTNAME=plex-server

# ByParr Configuration
LOG_LEVEL=info
LOG_HTML=false
CAPTCHA_SOLVER=none
PORT=8191 